/**index.wxss**/
.H-title{
  font-size: 32rpx;
  margin-top: 30rpx;
  text-align: center;
}
.H-title text{
  font-size: 32rpx;
  color: #FD7A00;
}
.H-text{
  text-align: center;
  font-size: 24rpx;
  margin-top: 10rpx;
  margin-bottom: 20rpx;
}
.main{
  width: 90%;
  margin: 0 auto;
  border: 1px solid #FD7A00;
  border-radius: 20rpx;
  padding-left: 20rpx;
  padding-right: 20rpx;
}
.list{
  display: flex;
  flex-direction: column;
}
.input{
  border: 1px solid #eee;
  display: flex;
  flex-direction: row;
  padding: 10rpx 0;
  margin-top: 20rpx;
}
.input view:first-child{
  width: 100rpx;
  text-align: center;
  display: flex;
  align-items: center;
  justify-content: center;
}
.input view:nth-child(2){
  border-left: 1px solid #ccc;
}
.input view:nth-child(3){
  width: 200rpx;
  font-size: 24rpx;
  text-align: center;
  display: flex;
  align-items: center;
  padding-left: 20rpx;

}
.search{
  position: absolute;
  right: 10rpx;
  width: 40rpx;
  height: 40rpx;
  top:5rpx;
}
.input view:last-child{
  position: relative;
  flex: 1;
  font-size: 24rpx;
  /*text-align: center;*/
}

.image1{
  width: 40rpx;
  height: 40rpx;
}
.image2{
  width: 50rpx;
  height: 40rpx;
}
.image3{
  width: 30rpx;
  height: 40rpx;
}
.agreement{
  display: flex;
  flex-direction: row;
  align-items: center;
}
.check{
  border: 1px solid #FD7A00;
  width: 30rpx;
  height: 30rpx;
  background: #eee;
  border-radius: 8rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.check image{
  width: 28rpx;
  height: 28rpx;
}

.agr_text{
  margin: 30rpx 20rpx;
  font-size: 24rpx;
}
.agr_text text{
  text-decoration: underline;
  color:#FD7A00;
}
button{
  width: 97%;
  color:#fff;
  background: #FD7A00;
  font-size: 32rpx;
}
.disable{
  background: #cccccc;
}
.hint{
  margin-top: 40rpx;
  display: flex;
  flex-direction: row;
  margin-bottom: 50rpx;
}
.h_image image{
  width: 30rpx;
  height: 30rpx;
}
.h_word{
  margin-left: 10rpx;
  font-size: 28rpx;
  color:#666
}

.fix{
  width: 100%;
  height: 100%;
  z-index: 100;
  position: absolute;
  top:0;
  left: 0;
  /*display: flex;*/
  /*flex-direction:column;*/
  /*justify-content: center;*/
/*position: absolute;*/
  /*top:0;*/
  /*left: 0;*/
}
.fix image{
  margin: 0 auto;
  width: 98%;
  height: 2380rpx;
  border: 1px solid #FD7A00;
}
.fix_button{
  background-color: #FD7A00;
}
.modal{
  position: fixed;
  top:20%;
  left:10%;
  width: 80%;
  height: 400rpx;
  border: 1rpx solid #FD7A00;
  border-top-left-radius: 10rpx;
  border-top-right-radius: 10rpx;
}
input{
  z-index: 10;
}
.all_Modal{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: rgba(0,0,0,.1);

}
.modal_select{

  height: 100rpx;
  width: 100%;
  display: flex;
  justify-content: space-between;
  background: #FD7A00;
  color:#fff;
}
.modal_select text{
  width: 100rpx;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  font-size: 32rpx;
}
.item{
  display: flex;
  flex-wrap:wrap;
  flex-direction: row;
}
.short_input{
  width: 280rpx;
}
.item view{
  width: 33.3333333%;
  box-sizing: border-box;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 24rpx;
  text-align: center;
  align-items: center;
  background: #fff;
  border: 1rpx solid #FD7A00;
}

